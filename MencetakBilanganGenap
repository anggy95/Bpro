import java.util.*;
public class MencetakBilanganGenap
{
	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Jumlah Bilangan Genap : ");
		int n = sc.nextInt();
		int i =0;
		int bilgenap = 0;
		while(i<n)
		{
			System.out.println("Bilangan genap ke : " +i +"=" +bilgenap);
			bilgenap +=2;
			i++;
		}
	}
}
